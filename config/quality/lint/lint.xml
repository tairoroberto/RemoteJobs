    <!-- Fail on all hardcoded text values in this project. -->
    <issue id="HardcodedText" severity="error" />

    <issue id="InvalidPackage" severity="warning" />

    <!-- Fail on missing click methods if wiring from XML. -->
    <issue id="OnClick" severity="error" />

    <!-- Fail on missing required super calls. -->
    <issue id="MissingSuperCall" severity="error" />

